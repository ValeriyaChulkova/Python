birth_years = [1987, 1992, 1995, 2001, 2005, 1982, 1999, 2002, 1990, 1988]
# Инициализация переменных для подсчета людей
before_1990 = 0
after_2000 = 0
# Цикл по списку годов рождения
for year in birth_years:
    if year < 1990:
        before_1990 += 1
    elif year > 2000:
        after_2000 += 1
print(f"Количество людей, родившихся до 1990 года: {before_1990}")
print(f"Количество людей, родившихся после 2000 года: {after_2000}")
