# заданные данные
rainfall = [5, 9, 3, 7, 6, 8, 2, 4, 1, 7, 5, 9, 4, 6, 3, 7, 8, 2, 5, 6, 9, 3, 1, 4, 8, 6, 2, 5, 3, 7]
# переменные для хранения количества осадков по четным и нечетным числам
even_rainfall = 0
odd_rainfall = 0
# перебор чисел от 1 до 28 и проверка четности
for day in range(1, 29):
    if day % 2 == 0:
        even_rainfall += rainfall[day - 1]
    else:
        odd_rainfall += rainfall[day - 1]
# сравнение количества осадков по четным и нечетным числам
if even_rainfall > odd_rainfall:
    print("По четным числам выпало больше осадков")
else:
    print("По нечетным числам выпало больше осадков")
