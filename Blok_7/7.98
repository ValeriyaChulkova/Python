numbers = [int(i) for i in input().split()] # вводим последовательность чисел
for i in range(len(numbers)-1):
    if numbers[i] % 2 != 0 and numbers[i+1] % 2 != 0: # если оба числа нечетные
        print("Найдена пара соседних нечетных чисел:", numbers[i], "и", numbers[i+1])
        break # выходим из цикла, т.к. нас интересует только первая пара
else:
    print("Соседних нечетных чисел не найдено") # если цикл отработал до конца и ничего не нашел.
