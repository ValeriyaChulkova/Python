python
grades = [
    [3, 4, 5, 4, 3],  # оценки первого ученика
    [5, 5, 4, 5, 5],  # оценки второго ученика
    [3, 2, 4, 3, 2],  # оценки третьего ученика
]
fives = 0  # количество пятерок
twos = 0   # количество двоек
for student in grades:  # перебор оценок каждого ученика
    for grade in student:  # перебор оценок ученика
        if grade == 5:  # если оценка равна 5
            fives += 1  # увеличиваем счетчик пятерок на 1
        elif grade == 2:  # если оценка равна 2
            twos += 1  # увеличиваем счетчик двоек на 1
print("Количество пятерок:", fives)
print("Количество двоек:", twos)
