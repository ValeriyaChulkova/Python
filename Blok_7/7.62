a = [2, 5, 8, 10, 12]  # пример списка
p = 6  # пример числа
k = 3  # пример числа
# а)
count_a = 0  # переменная для подсчета количества чисел, больших p
for ai in a:
    if ai > p:
        count_a += 1
print("Количество чисел ai, которые больше", p, ":", count_a)
# б)
count_b = 0  # переменная для подсчета количества чисел, оканчивающихся на 5
for ai in a:
    if ai % 10 == 5:
        count_b += 1
print("Количество чисел ai, которые оканчиваются на 5:", count_b)
# в)
count_c = 0  # переменная для подсчета количества чисел, кратных k
for ai in a:
    if ai % k == 0:
        count_c += 1
print("Количество чисел ai, которые кратны", k, ":", count_c)
