numbers = [3, 5, 7, 2, 9, 11]  # заданный набор чисел
has_even = False  # флаг для определения наличия четного числа
for number in numbers:
    if number % 2 == 0:  # проверка на четность
        has_even = True  # если число четное, устанавливаем флаг в True
        break  # прерываем цикл, так как нам достаточно наличия хотя бы одного четного числа
if has_even:
    print("В заданном наборе есть хотя бы одно четное число")
else:
    print("В заданном наборе все числа нечетные")
