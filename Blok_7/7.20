# объявляем переменные
gruzopodemnost = 1000 # грузоподъемность автомобиля в кг
massa1 = 500 # масса первого груза в кг
massa2 = 300 # масса второго груза в кг
massa3 = 200 # масса третьего груза в кг

# вычисляем общую массу всех грузов
obshaya_massa = massa1 + massa2 + massa3

# проверяем, не превышает ли общая масса грузов грузоподъемность автомобиля
if obshaya_massa <= gruzopodemnost:
print("Общая масса грузов не превышает грузоподъемность автомобиля")
else:
print("Общая масса грузов превышает грузоподъемность автомобиля")
