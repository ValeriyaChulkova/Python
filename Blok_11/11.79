results = [3, 1, 2, 3, 2, 2, 1, 3, 3, 1, 3, 2, 3, 2, 2, 1, 3, 2, 1, 2, 3]
win_count = 0
draw_count = 0
loss_count = 0
for result in results:
    if result == 3:
        win_count += 1
    elif result == 1:
        draw_count += 1
    elif result == 2:
        loss_count += 1
print("Number of wins:", win_count)
print("Number of draws:", draw_count)
print("Number of losses:", loss_count)
