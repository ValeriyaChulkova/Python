#a
def check_positive_limit(arr):
    count = 0
    for num in arr:
        if num > 0:
            count += 1
            if count > 5:
                return False
    return True
#b 
def check_multiple_limit(arr):
    count = 0
    for num in arr:
        if num <= 50.55:
            count += 1
    if count%4 == 0:
        return True
    else:
        return False
