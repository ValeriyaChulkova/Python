array = [5, 10, 25, 15, 30, 20, 7, 8, 14, 22]
sum_less_than_20 = sum(num for num in array if num <= 20)
print("Sum of elements less than or equal to 20:", sum_less_than_20)
a = 15
sum_greater_than_a = sum(num for num in array if num > a)
print("Sum of elements greater than", a, ":", sum_greater_than_a)
