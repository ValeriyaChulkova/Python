array = [-5, 20, 100, 150, 0, -30, 50]
# All non-negative elements
non_negatives = [num for num in array if num >= 0]
print("Non-negative elements:", non_negatives)
# All elements not exceeding 100
not_exceeding_100 = [num for num in array if num <= 100]
print("Elements not exceeding 100:", not_exceeding_100)
