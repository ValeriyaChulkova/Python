precipitation_data = [50, 60, 70, 80, 90, 100, 110, 120, 130, 140, 150, 160, 170, 180, 190]
average_precipitation = sum(precipitation_data) / len(precipitation_data)
for i in range(len(precipitation_data)):
    deviation = precipitation_data[i] - average_precipitation
    print("Year {}: Average Precipitation: {:.2f}mm, Deviation from Average: {:.2f}mm".format(i+1, average_precipitation, deviation))
