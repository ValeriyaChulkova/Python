scores = [67, 55, 46, 38, 30, 26, 21, 18, 14, 11, 9, 7, 5, 3, 2]

new_score = 1 # новое количество очков

scores.append(new_score) # добавляем новое количество очков в список

scores.sort(reverse=True) # сортируем список по убыванию

# выводим список команд с учетом дополнительной команды
for i in range(len(scores)):
    print('Team', i+1, 'Score:', scores[i])
